{
  "name": "electron-react-typescript-template",
  "productName": "electron-react-typescript-template",
  "version": "1.0.0",
  "description": "Electron React boilerplate",
  "homepage": "https://www.ivansaldano.com/",
  "main": "dist/index.js",
  "scripts": {
    "start": "parcel src/index.html --out-dir parcel-dist",
    "build": "parcel build src/index.html --out-dir dist/parcel-build --public-url ./",
    "dev": "concurrently -k \"yarn start\" \"yarn:electron\"",
    "electron": "wait-on tcp:1234 && tsc && copyfiles -f \"electron/assets/**/*\" \"dist/assets\" && electron .",
    "package": "yarn build && tsc && electron-builder --",
    "release": "yarn build && electron-builder --publish=always",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "Ivan Saldano",
    "email": "ivanms1@gmail.com"
  },
  "license": "MIT",
  "build": {
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "directories": {
      "output": "out"
    },
    "appId": "conun.app",
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "artifactName": "out",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "category": "Utility",
      "target": "deb"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@parcel/transformer-typescript-tsc": "^2.0.0-alpha.3",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "autoprefixer": "9.8.6",
    "concurrently": "^6.0.0",
    "copyfiles": "^2.4.1",
    "electron": "^11.3.0",
    "electron-builder": "^22.10.5",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-react": "^7.22.0",
    "parcel-bundler": "1.12.3",
    "postcss-modules": "3.2.2",
    "sass": "^1.32.8",
    "typescript": "^4.2.3",
    "wait-on": "^5.2.1"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.4.1",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "electron-is-dev": "^2.0.0",
    "electron-serve": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "framer-motion": "^4",
    "ipfs-core": "^0.5.4",
    "jimp": "^0.16.1",
    "libp2p": "^0.30.10",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.3.1",
    "react-hook-form": "^6.15.5",
    "react-query": "^3.13.0",
    "react-router-dom": "^5.2.0"
  }
}
